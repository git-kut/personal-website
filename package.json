		"@slicemachine/adapter-sveltekit": "^0.3.58",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.15",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^8.56.10",
		"@types/yup": "^0.32.0",
		"autoprefixer": "^10.4.20",
		"concurrently": "^8.2.2",
